<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us"  Name="CustomCommandSet_en-us">
    <CommandPrefix> Custom Commands </CommandPrefix>
    <Example> Shut down, and whatever else I register later</Example>

<Command Name="ShutDownComputer">
    <Example> Shut down </Example>
    <ListenFor> I'm going to bed now </ListenFor>
    <ListenFor> Shut down </ListenFor>
    <Feedback> Okay, I'll power off now </Feedback>
    <Navigate />
</Command>
      
      
        </CommandSet>
